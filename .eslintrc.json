{
  "extends": ["next/core-web-vitals", "plugin:jsx-a11y/strict"],
  "plugins": ["jsx-a11y"],
  "rules": {
    "camelcase": "error",
    "no-duplicate-imports": "error",
    "no-console": "off",
    "no-alert": "error",
    "react/no-unknown-property":"off",
    "react/no-unescaped-entities ":"off",
    "padding-line-between-statements": "off"
  }
}
